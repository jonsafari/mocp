clear_playlist =	C
add_directory =		A
volume_down_1 =		<
volume_up_1 =		>
volume_down_5 =		,
volume_up_5 =		.
seek_forward =		RIGHT
seek_backward =		LEFT
seek_forward_fast =	]
seek_backward_fast =	[
