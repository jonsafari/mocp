refresh =		^r
reload =		r
toggle_hidden_files =	H
go_to_music_directory =	m
delete_from_playlist =	d
search_menu =		g /
save_playlist =		V
toggle_show_time =	^t
toggle_show_format =	^f
go_to_playing_file =	G
go_to_a_directory =	i
go_up =			U
