#: main.c:285
#, c-format
msgid ""
"Usage:\n"
"%s [OPTIONS]... [FILE]...\n"
"-V --version           Print program version and exit.\n"
"-h --help              Print usage and exit.\n"
msgstr ""
"Použití:\n"
"%s [PŘEPÍNAČ]… [SOUBOR]…\n"
"-V --version           Vypíše verzi programu a skončí.\n"
"-h --help              Vypíše návod na použití a skončí.\n"

#: main.c:292
#, c-format
msgid "-D --debug             Turn on logging to a file.\n"
msgstr "-D --debug             Zapne protokolování do souboru.\n"

#: main.c:296
#, c-format
msgid ""
"-S --server            Run only the server.\n"
"-F --foreground        Run server in foreground, log to stdout.\n"
"-R --sound-driver NAME Use the specified sound driver (oss, alsa, jack, "
"null).\n"
"-m --music-dir         Start in MusicDir.\n"
"-a --append            Append the files/directories/playlists passed in\n"
"                       the command line to playlist and exit.\n"
"-c --clear             Clear the playlist and exit.\n"
"-p --play              Start playing from the first item on the playlist.\n"
"-l --playit            Play files given on command line without modifing "
"the\n"
"                       playlist.\n"
"-s --stop              Stop playing.\n"
"-f --next              Play next song.\n"
"-r --previous          Play previous song.\n"
"-x --exit              Shutdown the server.\n"
"-T --theme theme       Use selected theme file (read from ~/.moc/themes if\n"
"                       the path is not absolute.\n"
"-C --config FILE       Use the specified config file instead of the "
"default.\n"
"-M --moc-dir DIR       Use the specified MOC directory instead of the "
"default.\n"
"-P --pause             Pause.\n"
"-U --unpause           Unpause.\n"
"-G --toggle-pause      Toggle between play/pause.\n"
"-v --volume (+/-)LEVEL Adjust PCM volume.\n"
"-y --sync              Synchronize the playlist with other clients.\n"
"-n --nosync            Don't synchronize the playlist with other clients.\n"
"-A --ascii             Use ASCII characters to draw lines.\n"
"-i --info FORMAT       Print the information about the currently played "
"file.\n"
"-Q --format            Print the formatted information about the currently "
"played file.\n"
"-e --recursively       Alias for -a.\n"
"-k --seek N            Seek by N seconds (can be negative).\n"
"-o --on <controls>     Turn on a control (shuffle,autonext,repeat).\n"
"-u --off <controls>    Turn off a control (shuffle,autonext,repeat).\n"
"-t --toggle <controls> Toggle a control (shuffle,autonext,repeat).\n"
msgstr ""
"-S --server            Spustí jen server.\n"
"-F --foreground        Spustí server na popředí, protokol vypisuje na\n"
"                       standardní výstup.\n"
"-R --sound-driver NÁZEV\n"
"                       Použije zadaný zvukový ovladač (oss, alsa, jack, "
"null).\n"
"-m --music-dir         Začne v adresáři s hudbou (MusicDir).\n"
"-a --append            Připojí soubory/adresáře/seznamy skladeb uvedené na\n"
"                       příkazovém řádku k seznamu skladeb a skončí.\n"
"-c --clear             Vyprázdní seznam skladeb a skončí.\n"
"-p --play              Začne hrát od první položky na seznamu skladeb.\n"
"-l --playit            Přehraje soubory uvedené na příkazové řádce bez "
"změny\n"
"                       seznamu skladeb.\n"
"-s --stop              Přestane hrát.\n"
"-f --next              Přehraje další skladbu.\n"
"-r --previous          Přehraje předchozí skladbu.\n"
"-x --exit              Vypne server.\n"
"-T --theme TÉMA        Použije vybraný soubor s TÉMATEM (čte z ~/.moc/"
"themes,\n"
"                       pokud cesta není absolutní\n"
"-C --config SOUBOR     Použije zadaný konfigurační SOUBOR místo "
"implicitního.\n"
"-M --moc-dir ADRESÁŘ   Použije zadaný MOC ADRESÁŘ místo implicitního.\n"
"-P --pause             Pozastaví přehrávání.\n"
"-U --unpause           Pokračuje v pozastaveném přehrávání.\n"
"-G --toggle-pause      Přepne mezi hrát/pozastavit.\n"
"-v --volume (+/-)ÚROVEŇ\n"
"                       Upraví hlasitost PCM.\n"
"-y --sync              Seřídí seznam skladeb s ostatními klienty.\n"
"-n --nosync            Neseřídí seznam skladeb s ostatními klienty.\n"
"-A --ascii             Vykresluje čáry pomocí ASCII znaků.\n"
"-i --info FORMÁT       Vypíše informace o právě přehrávaném souboru.\n"
"-Q --format            Vypíše formátované informace o právě přehrávaném\n"
"                       souboru.\n"
"-e --recursively       Stejné jako -a.\n"
"-k --seek N            Převine o N sekund (může být záporné).\n"
"-o --on <FUNKCE>       Zapne FUNKCI („shuffle“ (náhodná skladba), "
"„autonext“\n"
"                       (přecházet na další skladbu), „repeat“ (opakovat)).\n"
"-u --off <FUNKCE>      Vypne FUNKCI („shuffle“, „autonext“, „repeat“).\n"
"-t --toggle <FUNKCE>   Přepne FUNKCI („shuffle“, „autonext“, „repeat“).\n"
